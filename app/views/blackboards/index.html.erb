<% @blackboards.each do |b| %>

<div class="col-md-4">
    <div class="panel panel-default">
        <div class="panel-heading">
            <h4 style="text-align: center"><%= b.name %></h4>
        </div>
        <div class="panel-body">
            <%= image_tag b.thumbnail, style: "width: 200px; height: 160px;" %>
        </div>
    </div>
</div>

<% end %>
